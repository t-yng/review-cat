    "@vanilla-extract/css": "^1.3.0",
    "classnames": "^2.3.1",
    "cross-fetch": "^3.1.4",
    "destyle.css": "^2.0.2",
    "jotai": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.5.0",
    "@testing-library/jest-dom": "^5.14.1",
