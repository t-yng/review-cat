    "@vanilla-extract/vite-plugin": "^2.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-custom": "*",
    "graphql": "^15.5.1",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
